int main() {

  node n1;
  n1 = create_node(50.0, 50.0, 10.0, 0, 0, 0, 0);
  node n2;
  n2 = create_node(150.0, 40.0, 10.0, 255, 255, 0, 0);

  edge e12;
  e12 = create_edge(n1, n2, 3, 100, 0, 100);
  edge e21;
  e21 = create_edge(n2, n1, 3, 100, 0, 100);

  graph g;
  g = create_graph();
  
  add_node(g, n1);
  add_node(g, n2);
  add_edge(g, e12);
  add_edge(g, e21);

  show_graph(g);
  destroy_graph(g);
  return 0;
}