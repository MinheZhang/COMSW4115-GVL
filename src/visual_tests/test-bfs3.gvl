// bfs

int bfs(graph g, node n) {
  list queue;
  queue = create_list();
  insert_back(queue, n);

  set_node_color(n, 100, 100, 100);
  show_graph(g);
  node newn;
  while(is_empty(queue) == 0){
    newn = remove_front(queue);
    list adjs;
    adjs = get_edges(g, newn);
    list_iterator iter;
    for (iter = list_begin(adjs); iter != list_end(); iter = list_iter_next(iter)){
        edge e;
        e = list_iter_data(iter);
        node end_node;
        end_node = get_edge_end(e);
        if (get_node_r(end_node) != 100){
            set_node_color(end_node, 100, 100, 100);
            show_graph(g);
            insert_back(queue, end_node);
        }
    }
  }
}
